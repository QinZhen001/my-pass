{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "noUnusedLocals": true,
    "esModuleInterop": true
  }
}
